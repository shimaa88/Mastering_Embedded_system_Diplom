#include "stdio.h"

#define SIZE   20
	int main()
	{int arr[SIZE][SIZE];
	int t[SIZE][SIZE];
	int r,c;
	printf("Enter rows and columns of matrix:");
	fflush(stdout);
	scanf("%d%d",&r,&c);
	printf("Enter elements of matrix:");
	fflush(stdout);
	for(int i=0;i<r;i++)
	{for(int j=0;j<c;j++)
	{
		printf("Enter elements a%d%d: ",i,j);
		fflush(stdout);
		scanf("%d",&arr[i][j]);

	}}

		for(int i=0;i<r;i++)
		{for(int j=0;j<c;j++)
		{
			t[i][j]=arr[j][i];
		}}
		printf("Transpose of Matrix");
			fflush(stdout);
				for(int i=0;i<r;i++)
				{for(int j=0;j<c;j++)
				{
					printf("%d\t",t[i][j]);
					fflush(stdout);


				}}


	}

